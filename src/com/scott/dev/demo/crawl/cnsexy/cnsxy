CREATE TABLE `album_info` (
    `id` INT(11) NULL DEFAULT NULL,
    `name` VARCHAR(512) NULL DEFAULT NULL,
    `url` VARCHAR(1024) NULL DEFAULT NULL
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
;


CREATE TABLE `model_photo` (
    `model_id` INT(20) NULL DEFAULT NULL,
    `img_id` INT(11) NULL DEFAULT NULL,
    `img_url` VARCHAR(1024) NULL DEFAULT NULL,
    `img_path` VARCHAR(1024) NULL DEFAULT NULL
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
;
